<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>User Data</title>
    <link rel="stylesheet" href="./styles.css"> 
</head>
<body>
    <h1>All Users from 'datausers' Collection</h1>
    <table>
        <thead>
            <tr>
                <th>Username</th>
                <th>User ID</th>
            </tr>
        </thead>
        <tbody>
            <% users.forEach(user => { %>
                <tr>
                    <td><%= user.username %></td>
                    <td><%= user._id %></td>
                </tr>
            <% }) %>
        </tbody>
    </table>
</body>
</html>
